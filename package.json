    "gatsby-plugin-robots-txt": "*",
    "gatsby-plugin-sharp": "*",
    "gatsby-plugin-sitemap": "*",
    "gatsby-plugin-styled-components": "*",
    "gatsby-remark-external-links": "*",
    "gatsby-remark-images": "*",
    "gatsby-source-filesystem": "*",
    "gatsby-transformer-remark": "*",
    "gatsby-transformer-sharp": "*",
    "prop-types": "*",
    "pure-react-carousel": "*",
